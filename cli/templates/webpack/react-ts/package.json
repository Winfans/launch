{
  "name": "launch",
  "version": "0.0.0",
  "scripts": {
    "watch": "npm run watch:qa",
    "build": "npm run build:prod && npm run build:zip",
    "watch:qa": "npm run build:qa",
    "build:qa": "cross-env ENV=qa webpack --config scripts/webpack.qa.config.ts",
    "build:prod": "cross-env ENV=prod webpack --config scripts/webpack.prod.config.ts",
    "build:zip": "node scripts/build-zip.mjs",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix --max-warnings 0",
      "prettier --write",
      "git add ."
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix",
      "git add ."
    ]
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^5.8.4",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "store2": "^2.14.2",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.19",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/archiver": "^5.3.2",
    "@types/chrome": "^0.0.235",
    "@types/lodash-es": "^4.17.9",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/speed-measure-webpack-plugin": "^1.3.4",
    "@types/webextension-polyfill": "^0.10.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "archiver": "^6.0.1",
    "babel-loader": "^9.1.3",
    "babel-preset-react": "^6.24.1",
    "chalk": "^5.3.0",
    "commitlint": "^17.7.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.8",
    "rollup-plugin-copy": "^3.5.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-resources-loader": "^1.5.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  }
}
